{
    "name": "ECD COMBO TOOL",
    "version": "3.4",
    "permissions": [
        "tabs",
        "storage",
        "http://*.44ux.com/*",
        "http://ppms.paipaioa.com/*",
        "http://buy.qq.com/*",
        "http://*.wanggou.com/*",
        "http://static.paipaiimg.com/*",
        "http://*.paipai.com/*"
    ],
    "background": {
        "scripts": ["jquery.js", "background.js"]
    },
    "browser_action": {
        "default_icon": {
            "19": "images/icon19.png",
            "38": "images/icon38.png"
        },
        "default_title": "ECD COMBO TOOL",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "http://buy.qq.com/*",
                "http://*.wanggou.com/*",
                "http://ppms.paipaioa.com/*",
                "http://static.paipaiimg.com/*",
                "http://*.paipai.com/*"
            ],
            "js": ["content_script.js"],
            "run_at": "document_end"
        }
    ],
    "icons": {
        "19": "images/icon19.png",
        "38": "images/icon38.png",
        "128": "images/icon128.png"
    },
    "manifest_version": 2
}
